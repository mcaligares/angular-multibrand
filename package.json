{
  "name": "angular-multibrand",
  "version": "1.0.0",
  "description": "Angular multibrand proyect using sass",
  "main": "README.md",
  "scripts": {
    "start": "concurrently -c \"bgBlue.white,bgRed.white,bgCyan.white\" \"npm:brand-a\" \"npm:brand-b\" \"npm:factory\"",
    "install": "concurrently -c \"bgBlue.white,bgRed.white,bgCyan.white\" \"npm:install:brand-a\" \"npm:install:brand-b\" \"npm:install:factory\"",
    "build": "concurrently -c \"bgBlue.white,bgRed.white,bgCyan.white\" \"npm:build:brand-a\" \"npm:build:brand-b\" \"npm:build:factory\"",
    "install:brand-a": "cd project-brand-a && npm i",
    "install:brand-b": "cd project-brand-b && npm i",
    "install:factory": "cd project-factory && npm i",
    "build:brand-a": "cd project-brand-a && npm run build",
    "build:brand-b": "cd project-brand-b && npm run build",
    "build:factory:brand-x": "cd project-factory && npm run build -- -c=brand-x",
    "build:factory:brand-y": "cd project-factory && npm run build -- -c=brand-y",
    "brand-a": "cd project-brand-a && npm start -- --port=4201",
    "brand-b": "cd project-brand-b && npm start -- --port=4202",
    "factory": "cd project-factory && npm start -- --port=4203",
    "factory:brand-x": "cd project-factory && npm start -- -c=brand-x",
    "factory:brand-y": "cd project-factory && npm start -- -c=brand-y"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mcaligares/angular-multibrand.git"
  },
  "keywords": [
    "angular"
  ],
  "author": "mcaligares@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mcaligares/angular-multibrand/issues"
  },
  "homepage": "https://github.com/mcaligares/angular-multibrand#readme",
  "dependencies": {
    "concurrently": "^5.0.2"
  }
}
